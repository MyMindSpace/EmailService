# Docker Environment Variables for Production
# Copy this file to .env.docker and fill in your values

NODE_ENV=production
PORT=3003

# Email Configuration
EMAIL_SERVICE=gmail
EMAIL_USER=your-production-email@gmail.com
EMAIL_APP_PASSWORD=your-production-app-password
EMAIL_FROM=MyMindSpace <your-production-email@gmail.com>

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100
